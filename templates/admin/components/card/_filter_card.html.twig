<div class="card">
    <div class="card-header">
        <h5 class="card-title mb-0">Filter</h5>
    </div>
    <div class="card-body">
        {{ form_start(filter_form) }}
        {{ form_errors(filter_form) }}

        {{ include('admin/components/_form_widgets.html.twig') }}

        <div class="row ">
            <div class="form-control-lg">
                <button type="submit" class="btn btn-primary btn mb-1 me-2">
                    Filter
                </button>
                <a href="{{ path(app.request.get('_route')) }}" class="btn btn-text mb-1">Cancel</a>
            </div>
        </div>

        {{ form_end(filter_form, {'render_rest': false}) }}
    </div>
</div>